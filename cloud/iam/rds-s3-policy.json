{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Effect": "Allow",
      "Action": [
        "rds:*",
        "ec2:DescribeSecurityGroups",
        "ec2:DescribeSubnets",
        "ec2:DescribeVpcs"
      ],
      "Resource": "*"
    },
    {
      "Effect": "Allow",
      "Action": [
        "s3:*"
      ],
      "Resource": [
        "arn:aws:s3:::med-docs-dev",
        "arn:aws:s3:::med-docs-dev/*",
        "arn:aws:s3:::med-docs-prod",
        "arn:aws:s3:::med-docs-prod/*"
      ]
    }
  ]
}
